{
  "id": "{{quobyte.framework-name}}",
  "cpus": {{quobyte.cpus}},
  "mem": {{quobyte.mem}},
  "instances": {{quobyte.instances}},
  "args": [],
  "env":{
    "ZOOKEEPER": "{{quobyte.zk}}",
    "MESOS_MASTER":"{{mesos.master}}",
    "PORT": "{{quobyte.port}}",
    "DOCKER_IMAGE": "{{quobyte.docker_image}}",
    "MESOS_DNS_DOMAIN": "{{quobyte.mesos-dns_domain}}",
    "FRAMEWORK_IMAGE": "quobyte/quobyte-mesos:{{mesos.version}}"
    },
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "quobyte/quobyte-mesos:{{mesos.version}}",
      "forcePullImage": true,
      "network": "HOST"
    }
  },
  "healthChecks": [
    {
      "path": "/v1/health",
      "portIndex": 0,
      "protocol": "HTTP",
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 3
    }
  ]
}
