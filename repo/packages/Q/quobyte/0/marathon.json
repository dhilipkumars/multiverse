{
  "id": "{{quobyte.framework-name}}",
  "cpus": "{{quobyte.cpus}}",
  "mem": "{{quobyte.mem}}",
  "instances": "{{quobyte.instances}}",
  "args": [],
  "env":{
    "ZOOKEEPER": "--zk={{quobyte.zk}}",
    "MESOS_MASTER":"--master={{mesos.master}}",
    "PORT": "--port={{quobyte.port}}",
    "DOCKER_IMAGE": "--docker_image={{quobyte.docker_image}}",
    "FRAMEWORK_IMAGE": "--framework_image=quobyte/quobyte-mesos:latest",
    "DATA_RESOURCES": "--data_resources={{quobyte.data_resources}}"
    },
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "quobyte/quobyte-mesos:latest",
      "forcePullImage": true,
      "network": "HOST"
    }
  },
  "healthChecks": [
    {
      "path": "/v1/health",
      "portIndex": 0,
      "protocol": "HTTP",
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 3
    }
  ]
}
