{
  "id": "{{quobyte.framework-name}}",
  "cmd": "/opt/quobyte-mesos --zk={{quobyte.zk}} --master={{mesos.master}} --port={{quobyte.port}} --docker_image={{quobyte.docker_image}} --mesos_dns_domain={{quobyte.mesos-dns_domain}} --framework_image='quobyte/quobyte-mesos:latest' --data_resources={{quobyte.data_resources}}",
  "cpus": {{quobyte.cpus}},
  "mem": {{quobyte.mem}},
  "instances": {{quobyte.instances}},
  "args": [],
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "quobyte/quobyte-mesos:latest",
      "forcePullImage": true,
      "network": "HOST"
    }
  },
  "healthChecks": [
    {
      "path": "/v1/health",
      "portIndex": 0,
      "protocol": "HTTP",
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 3
    }
  ]
}
